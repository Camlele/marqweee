{
  "id": "marqweee",
  "name": "Marqweee!",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "22.5.0"
    }
  ],
  "featureFlags": {
    "enableSWCSupport": true
  },
  "requiredPermissions": {
    "launchProcess": {
        "schemes": [
            "https"
        ]
    },
    "network": {
        "domains": [
            "https://github.com/Camlele/marqweee"
        ]
    }
  },
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "mainContent",
      "minimumSize": {
        "width": 163,
        "height": 300
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 163,
        "height": 360
      },
      "preferredFloatingSize": {
        "width": 163,
        "height": 360
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/dark.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/light.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ],
      "label": {
        "default": "Marqweee!"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium"
      ]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ]
    }
  ]
}